<%= form_with url: sign_in_path, scope: "user", local: :true do |f| %>
    <div class="field">
        <%= f.label :email %>
        <%= f.text_field :email %>
    </div>

    <div class="field">
        <%= f.label :password %>
        <%= f.password_field :password %>
    </div>

    
        <%= f.submit %>
    

<% end %>